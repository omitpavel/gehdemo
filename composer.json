{
    "name": "laravel/ibox",
    "type": "project",
    "description": "ibox Dashboards.",
    "keywords": [
        "framework",
        "laravel",
        "ibox Dashboards"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "adldap/adldap": "5.*",
        "brian2694/laravel-toastr": "^5.58",
        "cartalyst/sentinel": "^7.0",
        "guzzlehttp/guzzle": "^7.2",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "10.48",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.0",
        "laravelcollective/html": "^6.1",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/HelperCommon.php",
            "app/Helpers/HelperCommonMessages.php",
            "app/Helpers/HelperIboardsSymphony.php",
            "app/Helpers/HelperIboardsCamis.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
